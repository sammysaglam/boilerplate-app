  "scripts": {
    "start": "webpack serve",
    "dev": "npm run start",
    "build": "webpack",
    "analyze": "webpack --env analyze=true",
    "serve": "serve -p 3456 ./build",
    "serve:process:start": "pm2 start \"npm run serve\" && sleep 5",
    "serve:process:stop": "pm2 stop all",
    "lint": "eslint .",
    "test": "jest",
    "e2e": "npx playwright install && npx playwright test",
    "e2e:debug": "npx playwright install && npx playwright test --debug",
    "e2e:record": "npx playwright install && npx playwright codegen localhost:3456",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "intl:extract": "formatjs extract 'src/**/*.ts*' --out-file src/translations/extracted-messages.json"
  },
